za ubaciti film iz home da je napisan u edit:
<form #movieForm="ngForm" (ngSubmit)="onSubmit(movieForm.value)"  >
  <div>
    <label for="movie">Movie: </label>
    <input type="text" id="movie" name="movie" [(ngModel)]="movie.movie" required/>
  </div>
  <button type="submit" [disabled]="movieForm.invalid">Submit</button>
</form>

11:30
movie: IMovie = {
    id: 0,
    movie: '',
    theater: '',
    ticketsAvailable: 0,
    ticketPrice: 0,
    showDate: '',
    showTime: '',
  };
  constructor(private dataService: DataService, route: ActivatedRoute) {
    let id = route.snapshot.params['id'];
    this.movie = dataService.getMovieById(id);
  }
  onSubmit(formData: IMovie) {
    console.log(formData);
  }
}
